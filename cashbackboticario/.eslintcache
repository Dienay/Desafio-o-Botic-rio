> {\n        //   console.log(err.message)\n        // })\n\n      try {\n        setListContext(data.default)\n      } catch (error) {\n          console.log(error, \"Erro\")\n      }\n  }\n\n  return (\n    <ListContext.Provider\n      value={{listContext: listContext, setListContext: setListContext}}\n    >\n      <Router />\n    </ListContext.Provider>\n  );\n}\n\nexport default App;\n","/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/reportWebVitals.js",[],"/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/Router/Router.js",[],"/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/BalancePage/index.js",[],"/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/CreateUserPage/index.js",["106"],"import React from 'react';\nimport Header from '../Components/Header/index';\nimport { Container, Form } from '../GlobalStyles/GlobalStyle';\nimport Button from 'react-bootstrap/Button';\nimport Nav from 'react-bootstrap/Nav';\nimport useForm from '../Hooks/useForm';\nimport { useHistory } from 'react-router-dom';\nimport Menu from '../Components/Menu';\n\nfunction CreateUserPage() {\n  const history = useHistory()\n\n  const {form, onChange} = useForm({name: \"\", cpf: \"\", email: \"\", password: \"\"})\n\n  const handleInputChange = (event) => {\n    const {name, value} = event.target\n    onChange(name, value)\n  }\n\n  const createUser = () => {\n    if(!form.name || !form.cpf || !form.email || !form.password) {\n      return alert(\"Todos os campos são obrigatórios\")\n    }\n\n    if(form.email.indexOf(\"@\") === -1){\n        return alert(\"Email inválido\")\n    }\n\n    if(form.password.length < 6){\n      return alert(\"A senha deve ter no mínimo 6 characters\")\n    }\n\n    const body = {\n      \"name\": form.name,\n      \"cpf\": form.cpf,\n      \"email\": form.email,\n      \"password\": form.password\n    }\n\n    alert(\"Usuário criado com sucesso!\")\n    history.replace(\"/login\")\n  }\n\n  return (\n    <Container>\n      <Header />\n        <Form>\n          <h2>Cadastrar usuário</h2>\n          <fieldset>\n            <input\n              id=\"name\"\n              value={form.name}\n              onChange={handleInputChange}\n              type=\"text\"\n              name=\"name\" \n              required\n              placeholder=\"Nome\"\n            />\n            <input \n              id=\"cpf\"\n              value={form.cpf}\n              onChange={handleInputChange}\n              type=\"Number\"\n              name=\"cpf\" \n              required\n              placeholder=\"CPF\"\n            />\n            <input\n              id=\"email\"\n              value={form.email}\n              onChange={handleInputChange}\n              type=\"email\"\n              name=\"email\" \n              required\n              placeholder=\"E-mail\"\n            />\n            <input\n              id=\"password\"\n              value={form.password}\n              onChange={handleInputChange}\n              type=\"password\"\n              name=\"password\" \n              required\n              placeholder=\"Senha\"\n            />\n          </fieldset>\n          <Button onClick={createUser} variant=\"secondary\" size=\"lg\" block>\n            Cadastrar\n          </Button>\n          <Nav.Item>\n            <Nav.Link href=\"/Login\">Login</Nav.Link>\n          </Nav.Item>\n        </Form>\n        <Menu />\n    </Container>\n  )\n}\n\nexport default CreateUserPage;","/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/ListPage/index.js",[],"/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/LoginPage/index.js",["107"],"import React from 'react';\nimport { Container, Form } from '../GlobalStyles/GlobalStyle';\nimport Button from 'react-bootstrap/Button';\nimport Nav from 'react-bootstrap/Nav';\nimport { useHistory } from 'react-router-dom';\nimport useForm from '../Hooks/useForm';\nimport Header from '../Components/Header/index';\nimport Menu from '../Components/Menu';\n\nfunction LoginPage() {\n  const history = useHistory()\n\n  const {form, onChange} = useForm({email: \"\", password: \"\"})\n\n  const handleInputChange = (event) => {\n    const {name, value} = event.target\n    onChange(name, value)\n  }\n\n  const login = () => {\n    if(!form.email || !form.password) {\n      return alert(\"Todos os campos são obrigatórios\")\n    }\n\n    if(form.email.indexOf(\"@\") === -1){\n        return alert(\"Email inválido\")\n    }\n\n    if(form.password.length < 6){\n      return alert(\"A senha deve ter no mínimo 6 characters\")\n    }\n\n    const body = {\n      \"email\": form.email,\n      \"password\": form.password\n    }\n\n    history.replace(\"/\")\n  }\n  return (\n    <Container>\n      <Header />\n      <Form>\n        <h2>Login</h2>\n        <fieldset>\n          <input\n            id=\"email\"\n            value={form.email}\n            onChange={handleInputChange}\n            type=\"email\"\n            name=\"email\" \n            required\n            placeholder=\"E-mail\"\n          />\n          <input\n            id=\"password\"\n            value={form.password}\n            onChange={handleInputChange}\n            type=\"password\"\n            name=\"password\" \n            required\n            placeholder=\"Senha\"\n          />\n        </fieldset>\n        <Button onClick={login} variant=\"secondary\" size=\"lg\" block>\n          Entrar\n        </Button>\n        <Nav.Item>\n          <Nav.Link href=\"/criar-usuario\">Cadastar usuário</Nav.Link>\n        </Nav.Item>\n      </Form>\n      <Menu />\n    </Container>\n  )\n}\n\nexport default LoginPage;","/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/RegisterProductPage/index.js",[],"/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/BalancePage/styles.js",[],"/home/dienay/Seleções/Desafio-o-Boticario/cashbackboticario/src/ListPage/styles.js",[],"/home/dienay/Seleções/